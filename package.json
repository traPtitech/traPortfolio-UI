{
  "name": "traportfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run start-mock\"",
    "serve": "vite preview",
    "build": "vite build",
    "format": "prettier --check src",
    "fix:format": "prettier --write src",
    "lint": "eslint --ext .ts,.vue src",
