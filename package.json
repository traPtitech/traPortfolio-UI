{
  "name": "traportfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint --fix --cache --ext .ts,.vue src",
    "lint:nofix": "eslint --cache --ext .ts,.vue src",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:nofix": "prettier --cache --check \"src/**/*.{vue,ts}\"",
    "type-check": "vue-tsc --noEmit",
    "gen-api": "node scripts/generateApi.js",
    "start-mock": "prism mock -p 4010 -d https://raw.githubusercontent.com/traPtitech/traPortfolio/main/docs/swagger/traPortfolio.v1.yaml"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "ress": "^5.0.2",
    "vue": "^3.4.31",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.6",
    "ts-morph": "^23.0.0",
    "typescript": "^5.5.2",
    "vite": "^5.4.9",
    "vue-tsc": "^2.0.24"
  },
  "private": true,
  "type": "module"
}
